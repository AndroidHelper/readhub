<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.john.hub.dao">
    
    <insert id="saveProxy" parameterType="cn.john.hub.domain.Proxy">
        INSERT INTO proxy(ipAddr,port,anonymity,type,speed,connectTime)
        VALUES
        <foreach collection="list" item="proxy" separator=",">
            (#{proxy.ipAddr},#{proxy.port},#{proxy.anonymity},#{proxy.type},#{proxy.speed},#{proxy.connectTime})
        </foreach>
    </insert>
    
    <select id="get">
        
    </select>
</mapper>